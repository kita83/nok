{
  "name": "nok-ink-frontend",
  "version": "0.1.0",
  "description": "React Ink-based terminal frontend for nok virtual office",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.tsx",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "bin": {
    "nok-ink": "./dist/index.js"
  },
  "keywords": [
    "nok",
    "matrix",
    "terminal",
    "cli",
    "ink",
    "react"
  ],
  "author": "nok team",
  "license": "MIT",
  "dependencies": {
    "ink": "^4.4.1",
    "react": "^18.2.0",
    "matrix-js-sdk": "^31.5.1",
    "node-notifier": "^10.0.1",
    "node-wav": "^0.0.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}